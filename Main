# import all files
from HUI import HUI
from Login import Login
from Stacking_function import Stacking_boxes as SB
from Realsense import RealSenseImageProcessing as RS
#import library's
import threading 

Login_val = Login
thread1 = threading.Thread(target=HUI, daemon=False, args= (Login_val,))
# thread2 = threading.Thread(target=RS, daemon=True)
thread3 = threading.Thread(target=Login, daemon=True)

# thread2.start()
thread1.start()
thread3.start()
thread3.join()
while Login == True:
    thread1.join()
    if not thread1.is_alive():
        quit()
